<p id="notice"><%= notice %></p>

<h1>Listing Recommendeduniversities</h1>

<table>
  <thead>
    <tr>
      <th>Institution name</th>
      <th>Institution address</th>
      <th>Institution city</th>
      <th>Institution state</th>
      <th>Institution zip</th>
      <th>Institution web address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recommendeduniversities.each do |recommendeduniversity| %>
      <tr>
        <td><%= recommendeduniversity.institution_name %></td>
        <td><%= recommendeduniversity.institution_address %></td>
        <td><%= recommendeduniversity.institution_city %></td>
        <td><%= recommendeduniversity.institution_state %></td>
        <td><%= recommendeduniversity.institution_zip %></td>
        <td><%= recommendeduniversity.institution_web_address %></td>
        <td><%= link_to 'Show', recommendeduniversity %></td>
        <td><%= link_to 'Edit', edit_recommendeduniversity_path(recommendeduniversity) %></td>
        <td><%= link_to 'Destroy', recommendeduniversity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Recommendeduniversity', new_recommendeduniversity_path %>
